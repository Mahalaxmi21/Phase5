<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Insert</servlet-name>
  	<display-name>Insert</display-name>
  	<description></description>
  	<servlet-class>web.Insert</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>InsertData</servlet-name>
  	<display-name>InsertData</display-name>
  	<description></description>
  	<servlet-class>web.InsertData</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Insert</servlet-name>
  	<url-pattern>/Insert</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>InsertData</servlet-name>
  	<url-pattern>/InsertData</url-pattern>
  </servlet-mapping>
